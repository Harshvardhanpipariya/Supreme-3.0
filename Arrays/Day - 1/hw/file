// Online C++ compiler to run C++ program online


int main() {
    // Write C++ code here
    int arr[10]={0};
    for(int i=0;i<10;i++)
    {
        std::cout<<*(arr+i)<<" "<<i<<std::endl;
    }
std::cout<<sizeof(arr);
    return 0;
}

